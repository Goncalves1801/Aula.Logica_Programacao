programa {
  funcao inicio() {

  inteiro numero, positivo, negativo, zero
  escreva("Informe o numero: ")
  leia(numero)

  se (numero > 0){
    escreva("positivo")}

  senao se (numero < 0 ){
    escreva("negativo")}

  senao se(numero == 0){
    escreva("zero")
    
  }

  }
  }


    
    
    
  

